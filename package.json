{
  "name": "react-server-example-tsx",
  "version": "4.1.0",
  "description": "Boilerplate for isomorphic web app with React server-side rendering in TypeScript.",
  "main": "dist/server.js",
  "repository": "styfle/react-server-example-tsx",
  "scripts": {
    "start": "node dist/server.js",
    "build": "webpack",
    "watch": "webpack --watch",
    "prettier": "prettier \"./src/**/*.ts\" \"./src/**/*.tsx\"",
    "prettier:check": "npm run prettier -- --list-different",
    "prettier:write": "npm run prettier -- --write",
    "test": "tsc --outDir . --sourceMap false && tape tests/**/*.js",
    "release": "npm version patch && git push --follow-tags && npm publish"
  },
  "license": "MIT",
  "dependencies": {
    "compression": "^1.7.1",
    "css-loader": "^0.28.9",
    "gzip": "^0.1.0",
    "http-gzip": "^1.0.0",
    "http-gzip-maybe": "^1.0.0",
    "pump": "^3.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-masonry-infinite": "^1.2.2",
    "semantic-ui-react": "^0.77.2",
    "style-loader": "^0.20.1",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@types/node": "^8.5.2",
    "@types/react": "^16.0.31",
    "@types/react-dom": "^16.0.3",
    "@types/tape": "^4.2.31",
    "awesome-typescript-loader": "^3.4.1",
    "node-sass": "^4.7.2",
    "prettier": "1.10.2",
    "sass-loader": "^6.0.6",
    "source-map-loader": "^0.2.3",
    "tape": "^4.8.0",
    "typescript": "^2.6.2",
    "url-loader": "^0.6.2",
    "webpack": "^3.11.0",
    "webpack-node-externals": "^1.6.0"
  }
}
